{
  "description": "Altering addons-dev/stage authors API response to cause empty authors list",
  "manifest_version": 2,
  "name": "empty-authors",
  "version": "1.1",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/http-response",
  "icons": {
    "48": "pen.svg"
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://addons-dev.allizom.org/api/v3/addons/addon/*",
    "https://addons.allizom.org/api/v3/addons/addon/*"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "applications": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  }
}
